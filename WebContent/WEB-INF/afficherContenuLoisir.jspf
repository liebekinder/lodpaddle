<%@ page pageEncoding="UTF-8"%>
<%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c"%>

<c:if test="${not empty themeLoisir}"> 

	<c:forEach var="item" items="${themeLoisir.listeSousTheme}">

		<h6 style="text-align: left;">
			<img class="linkImg" alt=" " src="<c:out value="${item.picto}" />" />
			<span style=" border-bottom: solid white 1px;"><c:out value="${item.titre}" /> </span>
		</h6>
		<div>

			<ul>
				<c:forEach var="link" items="${item.entree}">
					<li>
						<a href="<c:out value="${link.lienUrl}" />">
						<c:out value="${ link.lienNom}" /> </a>
					</li>
				</c:forEach>
			</ul>
					<!--  <a href="./CreateWidget" style="display:block;width:100%;height:100%;">MaxiMoi200 </a> -->

		</div>
	</c:forEach>
</c:if>